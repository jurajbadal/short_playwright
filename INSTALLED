# k6
docker-compose up k6
docker-compose down k6
# initial playwright in subfolder locally 
npm init -y
npm init playwright@latest
# run playwright locally
npx playwright test
npx playwright show-report
npx playwright codegen
npx playwright test examples.spec.js --ui
npx playwright test examples.spec.js --debug


# running playwright with docker
docker-compose up playwright
docker-compose down playwright

# building docker images
docker-compose build

